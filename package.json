{
  "name": "azdo-marketplace",
  "version": "6.0.0-alpha.1",
  "description": "AzDo Marketplace",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessehouwing/azdo-marketplace.git"
  },
  "keywords": [
    "azure-devops",
    "azure-pipelines",
    "azure-devops-marketplace",
    "tfs",
    "azure-devops-server"
  ],
  "workspaces": [
    "packages/core",
    "packages/azdo-task",
    "packages/github-action"
  ],
  "scripts": {
    "initdev": "npm install --no-progress --force --no-update-notifier --legacy-peer-deps --no-fund",
    "build": "npm run build --workspaces --if-present",
    "build:clean": "npm run clean && npm run initdev && npm run build",
    "package:tasks": "tfx extension create --root . --output-path dist --manifest-globs vss-extension.json",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --no-coverage",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs:validate-samples": "node Scripts/validate-doc-yaml-samples.mjs",
    "sync:action-inputs": "node Scripts/sync-action-inputs.mjs",
    "sync:action-inputs:check": "node Scripts/sync-action-inputs.mjs --check",
    "bundle:azdo": "node Scripts/bundle.mjs azdo",
    "bundle:actions": "node Scripts/bundle.mjs actions",
    "bundle": "npm run bundle:azdo && npm run bundle:actions",
    "setup:windows:path": "powershell -NoProfile -ExecutionPolicy Bypass -File ./Scripts/setup-windows-path.ps1",
    "clean": "git clean -fdX"
  },
  "author": "Jesse Houwing",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.13",
    "@types/yauzl": "^2.10.3",
    "@types/yazl": "^3.3.0",
    "eslint": "^10.0.1",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "^3.4.2",
    "rollup": "^4.59.0",
    "tfx-cli": "^0.23.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "yauzl": "^3.2.0",
    "yazl": "^3.3.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.57.1",
    "@rollup/rollup-win32-x64-gnu": "^4.57.1",
    "@rollup/rollup-win32-x64-msvc": "^4.57.1"
  }
}
